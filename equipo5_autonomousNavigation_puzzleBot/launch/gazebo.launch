<?xml version="1.0" ?>
<launch>

    <arg name="world_file" default="$(find equipo5_autonomousNavigation_puzzleBot)/worlds/ADJIC_map.world"/>

    <arg name="robot" default="$(find equipo5_autonomousNavigation_puzzleBot)/urdf/robot.urdf"/> 

    <param name="robot_description" command="cat $(arg robot)" /> 

    <param name="use_sim_time" value="true"/>

    <node name="gazebo" pkg="gazebo_ros" type="gazebo" args="$(arg world_file)" respawn="false" output="screen"/>
</launch>
